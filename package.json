{
  "name": "treebank-react",
  "version": "0.0.1",
  "description": "Treebank renderer for React",
  "homepage": "https://perseids-tools.github.io/treebank-react/",
  "keywords": [
    "treebank",
    "classics",
    "ancient greek",
    "latin",
    "perseids"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perseids-tools/treebank-react.git"
  },
  "bugs": {
    "url": "https://github.com/perseids-tools/treebank-react/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "test": "react-scripts test",
    "lint": "eslint --ext=js --ext=jsx .",
    "demo": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "prop-types": "^15.0.0"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "d3": "^6.2.0",
    "dagre-d3": "^0.6.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "xml2js": "^0.4.23"
  },
 "eslintConfig": {
    "extends": "react-app"
 },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist"
  ]
}
